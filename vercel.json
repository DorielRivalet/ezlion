{
  "version": 2,
  "routes": [
    {
      "src": "/api/v0/monsters",
      "dest": "/api/v0/monsters/index.ts"
    },
    {
      "src": "/api/v0/monsters/(\\d+)",
      "dest": "/api/v0/monsters/[id]/index.ts"
    },
    {
      "src": "/api/docs",
      "dest": "/api/docs/index.ts"
    },
    {
      "src": "/api/docs/swagger.json",
      "dest": "/api/docs/swagger.json"
    }
  ]
}
