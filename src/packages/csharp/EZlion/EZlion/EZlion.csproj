<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0-windows</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>EZlion</Title>
	<PackageVersion>5.0.0-beta</PackageVersion>
	<Authors>Doriel Rivalet</Authors>
	<Description>A data retrieval and translation project for Monster Hunter Frontier Z</Description>
	<RepositoryUrl>https://github.com/DorielRivalet/ezlion.git</RepositoryUrl>
	<PackageProjectUrl>https://github.com/DorielRivalet/ezlion</PackageProjectUrl>
	<RepositoryType>git</RepositoryType>
	<PackageTags>monster-hunter;Monster Hunter;Monster Hunter Frontier Z; Monster Hunter Frontier;MHF;MHF-Z</PackageTags>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
	<GenerateDocumentationFile>True</GenerateDocumentationFile>
	<Copyright>Copyright (c) Doriel Rivalet 2023</Copyright>
  </PropertyGroup>
</Project>
