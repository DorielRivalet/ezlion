<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0-windows</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>EZlion</Title>
	<PackageVersion>1.0.1-alpha</PackageVersion>
	<Authors>Doriel Rivalet</Authors>
	<Description>A data retrieval and translation project for Monster Hunter Frontier Z</Description>
	<RepositoryUrl>https://github.com/DorielRivalet/ezlion</RepositoryUrl>
	<PackageTags>monster-hunter</PackageTags>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
	<GenerateDocumentationFile>True</GenerateDocumentationFile>
	<Copyright>Copyright (c) Doriel Rivalet 2023</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

</Project>
