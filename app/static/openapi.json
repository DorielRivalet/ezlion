{
	"openapi": "3.1.0",
	"info": {
		"title": "EZlion API Reference",
		"version": "1.1.0",
		"summary": "Retrieves information about Monster Hunter Frontier Z.",
		"description": "This API returns a list of objects or a single object depending on the endpoint. Each list includes metadata information. All information comes from the game Monster Hunter Frontier Z. The rate-limit is 30 requests per minute and the responses are cached for 1 day in the server, assuming that there aren't any new deployments. Testing a GET and returning the whole list may be slow at first because of lack of cache.",
		"license": {
			"name": "MIT",
			"url": "https://github.com/DorielRivalet/ezlion/blob/main/LICENSE"
		}
	},
	"components": {
		"parameters": {
			"sortParameter": {
				"name": "sort",
				"in": "query",
				"description": "The category to sort by.",
				"required": false,
				"schema": {
					"type": "string",
					"default": "id"
				}
			},
			"orderParameter": {
				"name": "order",
				"in": "query",
				"description": "The order of the sorting.",
				"required": false,
				"schema": {
					"type": "string",
					"default": "ascending"
				}
			},
			"pageParameter": {
				"name": "page",
				"in": "query",
				"description": "The page number of the results to return. Does nothing if not setting the limit.",
				"required": false,
				"schema": {
					"type": "integer",
					"default": 1
				}
			},
			"limitParameter": {
				"name": "limit",
				"in": "query",
				"description": "The number of results to return per page. Returns all values if unset.",
				"required": false,
				"schema": {
					"type": "integer",
					"default": 10
				}
			}
		}
	},
	"paths": {
		"/api/v0/monsters": {
			"get": {
				"summary": "Get all monsters",
				"tags": ["monster"],
				"responses": {
					"200": {
						"description": "A list of monsters in the results key. Includes metadata key."
					}
				}
			},
			"parameters": [
				{ "$ref": "#/components/parameters/sortParameter" },
				{ "$ref": "#/components/parameters/orderParameter" },
				{ "$ref": "#/components/parameters/pageParameter" },
				{ "$ref": "#/components/parameters/limitParameter" }
			]
		},
